    /* Hide the default checkbox */ 
    .orange-checkbox input[type=checkbox] { 
      visibility: hidden; 
    } 

    /* Creating a custom checkbox 
    based on demand */ 
    .orange-checkbox .geekmark { 
      position: absolute; 
      top: 4px;
      left: 0;
      height: 15px;
      width: 15px;
      background-color: none; 
      border:1px solid #000;
    } 
    .orange-checkbox input:active ~ .geekmark { 
      background-color: #ff6e2e;
      border:1px solid #ff6e2e;
    } 

    /* Specify the background color to be 
    shown when checkbox is checked */ 
    .orange-checkbox input:checked ~ .geekmark { 
      background-color: #ff6e2e; 
      border:1px solid #ff6e2e;
    } 

    /* Checkmark to be shown in checkbox */

    /* It is not be shown when not checked */ 
    .orange-checkbox .geekmark:after { 
      content: ""; 
      position: absolute; 
      display: none; 
    } 

    /* Display checkmark when checked */ 
    .orange-checkbox input:checked ~ .geekmark:after { 
      display: block; 
    } 

    /* Styling the checkmark using webkit */ 

    /* Rotated the rectangle by 45 degree and  
    showing only two border to make it look 
    like a tickmark */ 
    .orange-checkbox .geekmark:after { 
      left: 3px;
      bottom: 2px;
      width: 6px; 
      height: 12px; 
      border: solid #000; 
      border-width: 0 2px 2px 0;
      -webkit-transform: rotate(45deg); 
      -ms-transform: rotate(45deg); 
      transform: rotate(45deg); 
    } 
