<div class="custom_second" data-section="homepage-ready-to-outdoor">
  <div class="feature-row">
    <div class="custom_second_wrapper">
      {% for images in object.metafields.product-benefits.images %}
      {% assign image = blank %}
      {% assign image = images[0] %}
      {% if image == blank and forloop.index == 1 %}
      	{% assign image = images %}
      {% endif %}
      {% if image != blank %}
      {% capture srcset %}{% for i in (1..10) %}{% assign width = 180 | times:i %}{{image.cloudinary_src | append:'w_' | append:width }} {{width}}w{% unless forloop.last %}, {% endunless %}{% endfor %}{% endcapture %}
      <div class="img_wrpr img1">
        <div class="image-wrap " style="height: 0; padding-bottom: {{ 100 | divided_by: image.aspect_ratio }}%;">
          {%- assign img_url = image.cloudinary_src -%}
          <img class="lazyload"
               data-src="{{ img_url }}"
               data-widths="[360, 540]",
               data-srcset="{{srcset}}"
               data-aspectratio="{{ image.aspect_ratio }}"
               data-sizes="auto"
               alt="{{ image.alt }}">
          <noscript>
            <img class="lazyloaded" src="{{ image.cloudinary_src | append: 'w_540' }}" alt="{{image.alt }}">
          </noscript>
        </div>
        <div class="bottom-left-icon illustration cloud_sun">
          <img data-src="https://cdn.shopify.com/s/files/1/2423/6599/files/output-onlinepngtools.png?v=1623855839" class="lazyload"/>
        </div>
      </div>
      {% endif %}
      {% assign image = images[1] %}
      {% if image == blank and forloop.index == 2 %}
      {% assign image = images %}
      {% endif %}
      {% if image != blank %}
      {% capture srcset %}{% for i in (1..10) %}{% assign width = 180 | times:i %}{{image.cloudinary_src | append:'w_' | append:width }} {{width}}w{% unless forloop.last %}, {% endunless %}{% endfor %}{% endcapture %}
      <div class="img_wrpr img2">
        <div class="image-wrap " style="height: 0; padding-bottom: {{ 100 | divided_by: image.aspect_ratio }}%;">
          {%- assign img_url = image.cloudinary_src -%}
          <img class="lazyload"
               data-src="{{ img_url }}"
               data-widths="[360, 540]",
               data-srcset="{{srcset}}"
               data-aspectratio="{{ image.aspect_ratio }}"
               data-sizes="auto"
               alt="{{ image.alt }}">
          <noscript>
            <img class="lazyloaded" src="{{ image.cloudinary_src | append: 'w_540' }}" alt="{{image.alt }}">
          </noscript>
        </div>
      </div>
      {% endif %}
       {% assign image = images[2] %}
      {% if image == blank and forloop.index == 3 %}
      {% assign image = images %}
      {% endif %}
      {% if image != blank %}
      {% capture srcset %}{% for i in (1..10) %}{% assign width = 180 | times:i %}{{image.cloudinary_src | append:'w_' | append:width }} {{width}}w{% unless forloop.last %}, {% endunless %}{% endfor %}{% endcapture %}
      <div class="img_wrpr img3">
        <div class="image-wrap " style="height: 0; padding-bottom: {{ 100 | divided_by: image.aspect_ratio }}%;">
          {%- assign img_url = image.cloudinary_src -%}
          <img class="lazyload"
               data-src="{{ img_url }}"
               data-widths="[360, 540]",
               data-srcset="{{srcset}}"
               data-aspectratio="{{ image.aspect_ratio }}"
               data-sizes="auto"
               alt="{{ image.alt }}">
          <noscript>
            <img class="lazyloaded" src="{{ image.cloudinary_src | append: 'w_540' }}" alt="{{image.alt }}">
          </noscript>
        </div>
      </div>
      {% endif %}
      {% endfor %}
      <div class="center_content">
        <h2>{{ object.metafields.product-benefits.title }}</h2>
        {{ object.metafields.product-benefits.description }}
      </div>
    </div>
  </div>
</div>
